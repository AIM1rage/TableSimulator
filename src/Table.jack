class Table {
    static int x, y;
    static int size;

    function void init() {
        let x = 128;
        let y = 0;
        let size = 256;
        return;
    }

    function void printTable() {
        do Screen.setColor(true);
        do Screen.drawRectangle(x, y, x + size - 1, y + size - 1);
        do Screen.setColor(false);
        do Screen.drawRectangle(x + 1, y + 1, x + size - 2, y + size - 2);
        return;
    }
}